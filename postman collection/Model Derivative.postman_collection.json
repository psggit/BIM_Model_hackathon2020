									"raw": "{\n    \"input\": {\n        \"urn\": \"{{t6_ossEncodedSourceFileURN}}\",\n        \"rootFilename\": \"Stapler.iam\",\n        \"compressedUrn\": true\n    },\n    \"output\": {\n        \"destination\": {\n            \"region\": \"us\"\n        },\n        \"formats\": [\n            {\n                \"type\": \"svf\",\n                \"views\": [\n                    \"2d\",\n                    \"3d\"\n                ]\n            }\n        ]\n    }\n}",
