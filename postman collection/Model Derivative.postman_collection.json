										"exec": [
											"var body = JSON.parse(responseBody);",
											"pm.environment.set(\"t6_ossSourceFileObjectKey\", body[\"objectKey\"]);",
											"pm.environment.set(\"t6_ossSourceFileURN\", body[\"objectId\"]);",
											"var encoded_urn=btoa(body[\"objectId\"]);",
